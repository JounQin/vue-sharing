    "build": "cross-env NODE_ENV=production webpack --config build/webpack.config.babel.js --progress -p",
    "dev": "cross-env DEBUG=mic:* babel-node server",
    "start": "cross-env NODE_ENV=production npm run dev",
    "lint": "eslint src/vue/**/*.js .vue"
